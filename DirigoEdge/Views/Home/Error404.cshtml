@using System.Text
@using DirigoEdgeCore.Utils
@{
    ViewBag.Title = "Page Not Found";

    string htmlContent = "<h1>404</h1>";
    var page = CachedObjects.GetCacheContentPages().FirstOrDefault(x => x.Permalink == "404");

    if (page != null)
    {
        htmlContent = ContentUtils.GetFormattedPageContent(new StringBuilder(page.HTMLContent)).ToString();
    }
    
}

@Html.Raw(htmlContent)
